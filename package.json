{
  "name": "@gahojin-inc/holiday-japanese",
  "version": "2025.3.0",
  "description": "Japanese holidays",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/holiday-japanese.git"
  },
  "files": ["dist"],
  "keywords": ["holidays"],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "prepare": "husky",
    "clean": "rimraf dist",
    "prebuild": "tsx scripts/build.ts",
    "build": "tsup",
    "prepublish": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc",
    "check:packagejson": "attw --pack .",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage",
    "bench": "vitest bench",
    "ncu": "npm-check-updates -u"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.4",
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@holiday-jp/holiday_jp": "2.4.0",
    "@types/node": "20.17.28",
    "@vitest/coverage-v8": "3.0.9",
    "esbuild": "0.25.2",
    "husky": "9.1.7",
    "jest-extended": "4.0.2",
    "npm-check-updates": "17.1.16",
    "rimraf": "6.0.1",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.9",
    "yaml": "2.7.1"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=18.x",
    "npm": "use pnpm",
    "pnpm": ">=9.x",
    "yarn": "use pnpm"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "$esbuild"
    }
  }
}
