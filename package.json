{
  "name": "@gahojin-inc/holiday-japanese",
  "version": "2025.11.0",
  "description": "Japanese holidays",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/holiday-japanese.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "holidays"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "tsx scripts/build.ts && rimraf dist",
    "build": "rolldown -c",
    "prepublishOnly": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc",
    "check:packagejson": "attw --pack .",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage",
    "bench": "vitest bench",
    "ncu": "npm-check-updates -u"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.3.2",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@holiday-jp/holiday_jp": "2.5.1",
    "@types/japanese-holidays": "1.0.3",
    "@types/node": "24.10.0",
    "@vitest/coverage-v8": "4.0.6",
    "japanese-holidays": "1.0.10",
    "jest-extended": "6.0.0",
    "lefthook": "2.0.2",
    "npm-check-updates": "19.1.2",
    "rimraf": "6.1.0",
    "rolldown": "1.0.0-beta.46",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "unplugin-isolated-decl": "0.15.3",
    "vite": "7.1.12",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.6",
    "yaml": "2.8.1"
  }
}
