{
  "name": "@gahojin-inc/holiday-japanese",
  "version": "2025.3.0",
  "description": "Japanese holidays",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/holiday-japanese.git"
  },
  "files": ["dist"],
  "keywords": ["holidays"],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "clean": "rimraf dist",
    "prebuild": "tsx scripts/build.ts",
    "build": "pkgroll --clean-dist --sourcemap",
    "prepublish": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc",
    "check:packagejson": "attw --pack .",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage",
    "bench": "vitest bench",
    "ncu": "npm-check-updates -u"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "@arethetypeswrong/cli": "0.17.4",
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@holiday-jp/holiday_jp": "2.4.0",
    "@types/node": "20.17.30",
    "@vitest/coverage-v8": "3.1.2",
    "esbuild": "0.25.3",
    "jest-extended": "4.0.2",
    "lefthook": "1.11.11",
    "npm-check-updates": "18.0.0",
    "pkgroll": "2.12.1",
    "rimraf": "6.0.1",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.2",
    "yaml": "2.7.1"
  },
  "packageManager": "pnpm@10.8.1",
  "engines": {
    "node": ">=18.x",
    "npm": "use pnpm",
    "pnpm": ">=9.x",
    "yarn": "use pnpm"
  }
}
