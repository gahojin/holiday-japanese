{
  "name": "@gahojin-inc/holiday-japanese",
  "version": "2025.7.1",
  "description": "Japanese holidays",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/holiday-japanese.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "holidays"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "clean": "rimraf dist",
    "prebuild": "tsx scripts/build.ts && rimraf dist",
    "build": "rolldown -c",
    "prepublish": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc",
    "check:packagejson": "attw --pack .",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage",
    "bench": "vitest bench",
    "ncu": "npm-check-updates -u"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.1.2",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@holiday-jp/holiday_jp": "2.5.1",
    "@types/japanese-holidays": "1.0.3",
    "@types/node": "24.1.0",
    "@vitest/coverage-v8": "3.2.4",
    "japanese-holidays": "1.0.10",
    "jest-extended": "6.0.0",
    "lefthook": "1.12.2",
    "npm-check-updates": "18.0.1",
    "rimraf": "6.0.1",
    "rolldown": "1.0.0-beta.29",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "unplugin-isolated-decl": "0.14.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "yaml": "2.8.0"
  },
  "packageManager": "pnpm@10.13.1"
}
